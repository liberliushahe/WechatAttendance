<template name="indexItem">
<view class="article_head">
  <text>{{article.title}}</text>
</view>
<view class="article_middle">
  <image class="article_icon" src="{{article.url}}" binderror="errorFunction"></image>
</view>
<view class="article_footer">
  <text>{{article.date}} | {{article.count}}人阅读</text>
</view>
</template>
